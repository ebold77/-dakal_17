<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">
		<!-- Барааны шаардахын мульт компани дүрэм -->
		<record id="product_expense_multi_company_rule" model="ir.rule">
			<field name="name">Product Expense Multi Company Rule</field>
			<field name="model_id" search="[('model','=','product.expense')]" model="ir.model" />
			<field name="domain_force">['|',('company_id','=',False),('company_id', 'in', company_ids)]</field>
		</record>

		<!-- Барааны шаардахын мөрийн мульт компани дүрэм -->
		<record id="product_expense_line_multi_company_rule" model="ir.rule">
			<field name="name">Product Expense Line Multi Company Rule</field>
			<field name="model_id" search="[('model','=','product.expense.line')]" model="ir.model" />
			<field name="domain_force">['|',('company_id','=',False),('company_id', 'in', company_ids)]</field>
		</record>
	</data>
</odoo>