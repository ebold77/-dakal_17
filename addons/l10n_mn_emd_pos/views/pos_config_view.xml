<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record model="ir.ui.view" id="pos_config_form_view_insurance">
        <field name="name">pos.config.view.form.for.emd</field>
        <field name="model">pos.config</field>
        <field name="inherit_id" ref="l10n_mn_ebarimt_3_0.pos_config_form_view_ebarimt_3_0" />
        <field name="arch" type="xml">

            <xpath expr="//div[@id='ebarimt_div']" position="inside">
                <setting>
                    <h2>Mongolian Health Insurance</h2>
                    <group>
                        <field name="emd_url"/>
                        <field name="emd_partner_id"/>
                        <field name="emd_price_list_id"/>
                        <field name="novartis_merchant_tin"/>
                    </group>
                    <group>
                            <field name="user_name"/>
                            <field name="password"/>
                            <field name="user_id"/>
                        </group>
                </setting>
            </xpath>
        </field>
    </record>
</odoo>